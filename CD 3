
#include <stdio.h>

int main() {
    char ch, next;
    FILE *fp=fopen("input.c","r");

    while((ch=fgetc(fp))!=EOF) {
        if(ch==' '||ch=='\n'||ch=='\t') continue;

        if(ch=='/') {
            next=fgetc(fp);
            if(next=='/'||next=='*') continue;
        }
        printf("%c",ch);
    }
    fclose(fp);
}
